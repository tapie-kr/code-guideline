{
  "name": "@tapie-kr/code-guideline-react",
  "version": "0.0.1",
  "description": "TAPIE Code Style Standards - React Configuration",
  "scripts": {
    "build": "rimraf dist && tsup",
    "prepublish": "pnpm run build",
    "lint": "eslint . --fix",
    "lint:dry": "eslint . --fix-dry-run"
  },
  "keywords": [
    "tapie",
    "standards",
    "lint",
    "eslint",
    "typescript",
    "react"
  ],
  "peerDependencies": {
    "@tapie-kr/code-guideline-base": "^0.0.2",
    "eslint": "^9.19.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsup": "^8.3.6"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^8.22.0"
  },
  "author": {
    "name": "Taeyeong Kim",
    "email": "taeyeong.kim@algorix.io",
    "url": "https://github.com/cottons-kr"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tapie-kr/code-guideline.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  }
}
